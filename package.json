{
  "name": "studyroom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:web": "next dev -p 3000",
    "dev:lecture": "npm --prefix services/lecture-server run dev",
    "dev:realtime": "npm --prefix services/realtime run dev",
    "docker:up": "cd infra && docker compose up -d --build",
    "docker:up:realtime": "cd infra && docker compose --profile realtime up -d --build",
    "docker:down": "cd infra && docker compose down",
    "docker:logs": "cd infra && docker compose logs -f --tail=200",
    "build": "next build",
    "build:lecture": "npm --prefix services/lecture-server run build",
    "build:realtime": "npm --prefix services/realtime run build",
    "start": "next start -p 3000",
    "start:lecture": "npm --prefix services/lecture-server run start",
    "start:realtime": "npm --prefix services/realtime run start",
    "supabase:apply": "node scripts/supabase-apply-studyroom.mjs",
    "supabase:verify": "node scripts/verify-studyroom-supabase.mjs",
    "lint": "eslint"
  },
  "dependencies": {
    "@livekit/components-react": "^2.9.19",
    "@livekit/components-styles": "^1.2.0",
    "@supabase/supabase-js": "^2.95.3",
    "livekit-client": "^2.17.1",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-youtube": "^10.1.0",
    "socket.io-client": "^4.8.1",
    "socket.io-msgpack-parser": "^3.0.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/youtube": "^0.1.0",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
